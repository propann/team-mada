# Global
PROJECT_NAME=team-mada
LOCAL_UID=1000
LOCAL_GID=1000
TZ=UTC

# Networking
N8N_PORT=5678
N8N_WEBHOOK_PORT=5679
GRAFANA_PORT=3000
PROMETHEUS_PORT=9090
MOSQUITTO_PORT=1883
MQTT_TLS_PORT=8883
MQTT_PORT=1883
MINIO_API_PORT=9000
MINIO_CONSOLE_PORT=9001
QDRANT_PORT=6333
STRUDEL_PORT=7000
STRUDL_HTTP_PORT=8080
AI_PROXY_PORT=8081
EMBED_SERVICE_PORT=8082

# Images (pin, pas de `latest`)
N8N_IMAGE=n8nio/n8n:1.74.1
POSTGRES_IMAGE=postgres:16-alpine
REDIS_IMAGE=redis:7-alpine
QDRANT_IMAGE=qdrant/qdrant:v1.8.5
MINIO_IMAGE=quay.io/minio/minio:RELEASE.2024-06-18T01-10-18Z
MQTT_IMAGE=eclipse-mosquitto:2.0.18
PROMETHEUS_IMAGE=prom/prometheus:v2.52.0
GRAFANA_IMAGE=grafana/grafana:10.4.3
STRUDEL_IMAGE=ghcr.io/strudel-cc/strudel:0.10.0
AI_PROXY_IMAGE=nginx:1.27-alpine
EMBED_SERVICE_IMAGE=python:3.11-alpine

# Database
POSTGRES_USER=CHANGE_ME
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=n8n
POSTGRES_PORT_INTERNAL=5432
POSTGRES_BACKUP_DIR=./backups/postgres
REDIS_PASSWORD=CHANGE_ME
QDRANT_API_KEY=CHANGE_ME

# MinIO
MINIO_ROOT_USER=CHANGE_ME
MINIO_ROOT_PASSWORD=CHANGE_ME

# Mosquitto
MOSQUITTO_ALLOW_ANONYMOUS=false

# n8n
N8N_ENCRYPTION_KEY=CHANGE_ME
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=CHANGE_ME
N8N_DISABLE_PRODUCTION_MAIN_PROCESS=false
N8N_SECURE_COOKIE=true
N8N_WEBHOOK_URL=https://example.com/
N8N_DIAGNOSTICS_ENABLED=false

# Reverse proxy
PUBLIC_DOMAIN=example.com
EMAIL_LETSENCRYPT=ops@example.com
ENABLE_REVERSE_PROXY=false

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=CHANGE_ME

# AI proxy (HTTP only, clé à stocker côté secret manager)
GROQ_API_KEY=CHANGE_ME
